import React, { useState, useMemo } from 'react';
import { Card, CardContent, CardDescription, CardHeader, CardTitle } from './components/ui/card';
import { Button } from './components/ui/button';
import { Badge } from './components/ui/badge';
import { Tabs, TabsContent, TabsList, TabsTrigger } from './components/ui/tabs';
import { TankLevelWidget } from './components/TankLevelWidget';
import { TankDetail } from './components/TankDetail';
import { Analytics } from './components/Analytics';
import { AppSidebar } from './components/AppSidebar';
import { FlowMeters } from './components/FlowMeters';
import { ConnectionStatus } from './components/ConnectionStatus';
import { DebugPanel } from './components/DebugPanel';
import { WebSocketProvider, useWebSocket } from './contexts/WebSocketContext';
import { SidebarProvider, SidebarInset, SidebarTrigger } from './components/ui/sidebar';
import { Separator } from './components/ui/separator';
import { BarChart3, Droplets, Gauge, AlertTriangle, Settings, Activity, RefreshCw } from 'lucide-react';
import { TankData, FlowMeterData } from './services/apiService';
import { dataProcessingService } from './services/dataProcessingService';
import { TankSelectionProvider, useTankSelection } from './contexts/SystemStatusContext';

type ViewType = 'dashboard' | 'analytics' | 'flow-meters' | 'all-tanks' | 'tank-1' | 'tank-2' | 'tank-3' | 'tank-4' | 'pump-house' | 'debug';

// Main App Component that uses WebSocket Context
function AppContent() {
  const [viewType, setViewType] = useState<'rectangular' | 'cylindrical'>('rectangular');
  const [currentView, setCurrentView] = useState<ViewType>('dashboard');

  // Use WebSocket context for real-time data
  const {
    tanks,
    flowMeters,
    systemStatus,
    isConnected,
    error,
    retry,
    lastUpdate
  } = useWebSocket();

  const { selectTank } = useTankSelection();

  const handleTankClick = (tankId: string) => {
    selectTank(tankId);
    setCurrentView('analytics');
  };

  const handleRefresh = () => {
    retry();
  };

  // Use centralized processing for statistics calculation
  const { tankStats, flowStats } = useMemo(() => {
    return dataProcessingService.calculateSystemStatistics(tanks, flowMeters);
  }, [tanks, flowMeters]);

  // Get specific tank by ID
  const getTankById = (id: string): TankData | undefined => {
    return tanks.find(tank => tank.id === id);
  };

  const renderContent = () => {
    switch (currentView) {
      case 'analytics':
        return <Analytics tanks={tanks} />;
        
      case 'flow-meters':
        return <FlowMeters />;
        
      case 'all-tanks':
        return (
          <div className="space-y-6">
            <div className="flex items-center justify-between">
              <div>
                <h1 className="text-2xl font-bold">All Tanks Overview</h1>
                <p className="text-muted-foreground">Complete monitoring of all tank systems</p>
              </div>
              <div className="flex items-center gap-2">
                <Button
                  variant="outline"
                  size="sm"
                  onClick={() => setViewType(viewType === 'rectangular' ? 'cylindrical' : 'rectangular')}
                >
                  {viewType === 'rectangular' ? 'Cylindrical View' : 'Rectangular View'}
                </Button>
              </div>
            </div>
            
            <div className="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-4">
              {tanks.map(tank => (
                <TankLevelWidget key={tank.id} tank={tank} />
              ))}
            </div>
          </div>
        );
        
      case 'tank-1':
        const tank1 = getTankById('tank-1');
        return tank1 ? <TankDetail tank={tank1} viewType={viewType} /> : <div>Tank not found</div>;
        
      case 'tank-2':
        const tank2 = getTankById('tank-2');
        return tank2 ? <TankDetail tank={tank2} viewType={viewType} /> : <div>Tank not found</div>;
        
      case 'tank-3':
        const tank3 = getTankById('tank-3');
        return tank3 ? <TankDetail tank={tank3} viewType={viewType} /> : <div>Tank not found</div>;
        
      case 'tank-4':
        const tank4 = getTankById('tank-4');
        return tank4 ? <TankDetail tank={tank4} viewType={viewType} /> : <div>Tank not found</div>;
        
      case 'pump-house':
        const pumpHouse = getTankById('tank-5');
        return pumpHouse ? <TankDetail tank={pumpHouse} viewType={viewType} /> : <div>Tank not found</div>;
        
      case 'debug':
        return <DebugPanel />;
        
      case 'dashboard':
      default:
        return (
          <div className="space-y-6">
            {/* Dashboard Header */}
            <div className="grid grid-cols-1 md:grid-cols-4 gap-4">
              <Card>
                <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
                  <CardTitle className="text-sm font-medium">Total Tanks</CardTitle>
                  <Droplets className="h-4 w-4 text-muted-foreground" />
                </CardHeader>
                <CardContent>
                  <div className="text-2xl font-bold">{tankStats.totalTanks}</div>
                  <p className="text-xs text-muted-foreground">
                    {tankStats.activeTanks} active
                  </p>
                </CardContent>
              </Card>
              
              <Card>
                <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
                  <CardTitle className="text-sm font-medium">Available Capacity</CardTitle>
                  <Gauge className="h-4 w-4 text-muted-foreground" />
                </CardHeader>
                <CardContent>
                  <div className="text-2xl font-bold">{tankStats.totalCubicMetersAvailable.toFixed(1)}mÂ³</div>
                  <p className="text-xs text-muted-foreground">
                    Remaining capacity
                  </p>
                </CardContent>
              </Card>
              
              <Card>
                <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
                  <CardTitle className="text-sm font-medium">Flow Meters</CardTitle>
                  <Activity className="h-4 w-4 text-muted-foreground" />
                </CardHeader>
                <CardContent>
                  <div className="text-2xl font-bold">{flowStats.totalMeters}</div>
                  <p className="text-xs text-muted-foreground">
                    {flowStats.activeMeters} active
                  </p>
                </CardContent>
              </Card>
              
              <Card>
                <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
                  <CardTitle className="text-sm font-medium">Alerts</CardTitle>
                  <AlertTriangle className="h-4 w-4 text-muted-foreground" />
                </CardHeader>
                <CardContent>
                  <div className="text-2xl font-bold">{tankStats.alertCount}</div>
                  <p className="text-xs text-muted-foreground">
                    Active alerts
                  </p>
                </CardContent>
              </Card>
            </div>
            
            <div className="flex justify-between items-center">
              <h2 className="text-lg font-semibold">Tank Overview</h2>
              <Button
                variant="outline"
                size="sm"
                onClick={() => setViewType(viewType === 'rectangular' ? 'cylindrical' : 'rectangular')}
              >
                {viewType === 'rectangular' ? 'Cylindrical View' : 'Rectangular View'}
              </Button>
            </div>
            
            <div className="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-4">
              {tanks.length > 0 ? (
                tanks.map(tank => (
                  <TankLevelWidget key={tank.id} tank={tank} onTankClick={handleTankClick} />
                ))
              ) : (
                <div className="col-span-full flex flex-col items-center justify-center py-12 text-center">
                  <Droplets className="h-12 w-12 text-muted-foreground mb-4" />
                  <h3 className="text-lg font-medium text-muted-foreground mb-2">No Tank Data Available</h3>
                  <p className="text-sm text-muted-foreground max-w-md">
                    {!isConnected ? 
                      "Waiting for WebSocket connection to receive tank data..." : 
                      "Connected to WebSocket but no tank data received yet."
                    }
                  </p>
                </div>
              )}
            </div>
          </div>
        );
    }
  };

  const getPageTitle = () => {
    switch (currentView) {
      case 'analytics':
        return 'Analytics & Insights';
      case 'flow-meters':
        return 'Flow Meters Dashboard';
      case 'all-tanks':
        return 'All Tanks Overview';
      case 'tank-1':
        return 'Tank 1 Detail';
      case 'tank-2':
        return 'Tank 2 Detail';
      case 'tank-3':
        return 'Tank 3 Detail';
      case 'tank-4':
        return 'Tank 4 Detail';
      case 'pump-house':
        return 'Pump House Tank Detail';
      case 'dashboard':
      default:
        return 'Tank Level Monitoring';
    }
  };

  const getPageSubtitle = () => {
    switch (currentView) {
      case 'analytics':
        return 'Data analysis and performance metrics';
      case 'flow-meters':
        return 'Real-time flow monitoring and statistics';
      case 'all-tanks':
        return 'Complete system overview';
      case 'tank-1':
      case 'tank-2':
      case 'tank-3':
      case 'tank-4':
      case 'pump-house':
        return 'Individual tank monitoring and analytics';
      case 'dashboard':
      default:
        return 'Real-time IoT dashboard';
    }
  };

  return (
    <SidebarProvider>
      <AppSidebar onNavigate={setCurrentView} currentView={currentView} />
      <SidebarInset>
        <header className="flex h-16 shrink-0 items-center gap-2 border-b px-4">
          <SidebarTrigger className="-ml-1" />
          <Separator orientation="vertical" className="mr-2 h-4" />
          <div className="flex items-center gap-2 text-sm">
            <span className="font-semibold">{getPageTitle()}</span>
            <span className="text-muted-foreground">â¢</span>
            <span className="text-muted-foreground">{getPageSubtitle()}</span>
          </div>
        </header>
        
        <div className="flex flex-1 flex-col gap-3 p-4">
          <ConnectionStatus 
            wsConnected={isConnected}
            lastUpdate={lastUpdate}
            error={error}
          />
          {renderContent()}
        </div>
      </SidebarInset>
    </SidebarProvider>
  );
}

// Main App with WebSocket Provider
export default function App() {
  return (
    <WebSocketProvider 
      url="ws://localhost:1880/ws/data"
    >
      <TankSelectionProvider>
        <AppContent />
      </TankSelectionProvider>
    </WebSocketProvider>
  );
}
This Figma Make file includes components from [shadcn/ui](https://ui.shadcn.com/) used under [MIT license](https://github.com/shadcn-ui/ui/blob/main/LICENSE.md).

